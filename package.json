  "dependencies": {
    "basic-auth-header": "^1.0.1",
    "bluebird": "^3.4.0",
    "bower": "^1.7.9",
    "co-mmander": "^0.1.0",
    "commander": "^2.9.0",
    "deep-readdir": "^0.2.0",
    "immutable": "^3.8.1",
    "koa": "^1.2.0",
    "koa-router": "^5.4.0",
